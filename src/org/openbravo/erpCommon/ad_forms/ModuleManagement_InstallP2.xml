<REPORT>
  <template file="ModuleManagement_InstallP2.html"/>
    <PARAMETER id="messageBoxID" name="messageType" attribute="class" replace="yy" default="HIDDEN"/>
    <PARAMETER id="messageBoxIDTitle" name="messageTitle" default=""/>
    <PARAMETER id="messageBoxIDMessage" name="messageMessage" default=""/>
    <PARAMETER id="paramLanguage" name="language" default=""/>
  <PARAMETER id="paramKey" name="key" default="" attribute="value"/>
  <PARAMETER id="paramDirectory" name="directory" default=""/>
  <PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
  
  <structure name="selected">
    <FIELD id="modulename0">name</FIELD>
    <FIELD id="moduleversion0">versionNo</FIELD>
    <FIELD id="agree0" attribute="onfocus" replace="xx">moduleID</FIELD>
    <FIELD id="agree0" attribute="name" replace="xx">position</FIELD>
    <SECTION id="modulesSel"/>
  </structure>
  
  <structure name="installs">
    <FIELD id="modulename1">name</FIELD>
    <FIELD id="moduleversion1">versionNo</FIELD>
    <FIELD id="agree1" attribute="onfocus" replace="xx">moduleID</FIELD>
    <FIELD id="agree1" attribute="name" replace="xx">position</FIELD>
    <SECTION id="modulesInst"/>
  </structure>
  
  <structure name="updates">
    <FIELD id="modulaname2">name</FIELD>
    <FIELD id="moduleversion2">versionNo</FIELD>
    <FIELD id="agree2" attribute="onfocus" replace="xx">moduleID</FIELD>
    <FIELD id="agree2" attribute="name" replace="xx">position</FIELD>
    <SECTION id="modulesUp"/>
  </structure>
</REPORT>
