<REPORT>
  <template file="InstancePurpose.html"/>
  <PARAMETER id="paramWelcome" name="welcome" replaceCharacters="htmlHelp" />
  <PARAMETER id="paramDirectory" name="directory" default=""/>
  <PARAMETER id="paramLanguage" name="language" default=""/>
  <PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
  <PARAMETER id="paramCMD" name="cmd" default="var cmd='PROCEED';" />
  <PARAMETER id="paramRecordId" name="recordId" attribute="value" default="" replace="xx" />
  <PARAMETER id="paramTitle" name="title"/>
  <PARAMETER id="paramShowPurposeCombo" name="showPurposeCombo" default="Y" attribute="value"/>
  <PARAMETER id="paramShowButtonProceed" name="showButtonProceed" default="Y" attribute="value"/>
  <PARAMETER id="paramSelPurpose" name="selectedPurpose" default=""/>
  <PARAMETER id="paramNewTabTitle" name="newTabTitle" default="" replace="XX"/>
  <SUBREPORT id="reportPurpose" name="reportPurpose" report="org/openbravo/erpCommon/reference/List">
    <ARGUMENT name="parameterListSelected" withId="paramSelPurpose"/>
  </SUBREPORT>
</REPORT>
