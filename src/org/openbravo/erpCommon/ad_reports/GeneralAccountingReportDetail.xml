    <FIELD id="fieldQtyRef" replaceCharacters="fo" boolean="font-size='10pt'" withId="fieldLEVEL6">elementLevel</FIELD>
    <FIELD id="fieldElementLevel" replaceCharacters="fo">elementLevel</FIELD>
    <!--SECTION id="sectionLevel" field="elementLevel"/-->
    <SECTION id="sectionDetail"/>
