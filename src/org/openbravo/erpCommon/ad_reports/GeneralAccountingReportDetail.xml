  <PARAMETER id="fieldLEVEL2" name="level2" default="3"/>
  <PARAMETER id="fieldLEVEL3" name="level3" default="4"/>
  <PARAMETER id="fieldLEVEL4" name="level4" default="5"/>
  <PARAMETER id="fieldLEVEL5" name="level5" default="6"/>
  <PARAMETER id="fieldLEVEL6" name="level6" default="7"/>

  <structure name="structure1">
    <FIELD id="fieldName" replaceCharacters="fo">name</FIELD>
    <FIELD id="fieldName" replaceCharacters="fo" boolean="font-weight='bold' font-size='14pt'" withId="fieldLEVEL0">elementLevel</FIELD>
    <FIELD id="fieldName" replaceCharacters="fo" boolean="space-after='8pt' space-before.optimum='6pt'" withId="fieldLEVEL0">elementLevel</FIELD>
    <FIELD id="fieldName" replaceCharacters="fo" boolean="start-indent='4pt'" withId="fieldLEVEL0">elementLevel</FIELD>
