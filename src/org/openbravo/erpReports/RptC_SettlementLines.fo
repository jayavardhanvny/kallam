<DIVFO xmlns:fo="http://www.w3.org/1999/XSL/Format">
	<fo:block font-size="10pt">
		<fo:table>
			<fo:table-column column-width="2cm"/>
			<fo:table-column column-width="6cm"/>
			<fo:table-column column-width="4.5cm"/>
			<fo:table-column column-width="4.5cm"/>
			<fo:table-column column-width="2cm"/>
			<fo:table-header>
				<fo:table-row>
					<fo:table-cell>
						<fo:block font-weight="bold">Maturity date</fo:block>
					</fo:table-cell>
					<fo:table-cell>
						<fo:block font-weight="bold">Customer</fo:block>
					</fo:table-cell>
					<fo:table-cell>
						<fo:block font-weight="bold">CCC Bank</fo:block>
					</fo:table-cell>
					<fo:table-cell>
						<fo:block font-weight="bold">Invoice no/Debtpayment no</fo:block>
					</fo:table-cell>
					<fo:table-cell>
						<fo:block text-align="right" font-weight="bold">Amount</fo:block>
					</fo:table-cell>
				</fo:table-row>
			</fo:table-header>
			<fo:table-body>
				<DIVFO id="sectionDetailLines">
					<fo:table-row>
						<fo:table-cell>
							<fo:block font-size="8pt" start-indent="2pt" id="fieldDateplanned">XX</fo:block>
						</fo:table-cell>
						<fo:table-cell>
							<fo:block font-size="8pt" start-indent="2pt" id="fieldName">XX</fo:block>
						</fo:table-cell>
						<fo:table-cell>
							<fo:block font-size="8pt" start-indent="2pt" id="fieldAccount">XX</fo:block>
						</fo:table-cell>
						<fo:table-cell>
							<fo:block font-size="8pt" start-indent="2pt" id="fieldDescription">XX</fo:block>
						</fo:table-cell>
						<fo:table-cell>
							<fo:block font-size="8pt" start-indent="2pt" text-align="right" id="fieldAmount">XX</fo:block>
						</fo:table-cell>
					</fo:table-row>
				</DIVFO>
				<fo:table-row>
					<fo:table-cell number-columns-spanned="5">
						<fo:block>&#160;</fo:block>
					</fo:table-cell>
				</fo:table-row>
				<fo:table-row>
					<fo:table-cell number-columns-spanned="4">
						<fo:block text-align="right" font-weight="bold" font-size="12pt">Total:</fo:block>
					</fo:table-cell>
					<fo:table-cell>
						<fo:block font-weight="bold" text-align="right" id="fieldTotalNetAmount">xx</fo:block>
					</fo:table-cell>
				</fo:table-row>
				<fo:table-row>
					<fo:table-cell number-columns-spanned="4">
						<fo:block font-weight="bold" text-align="right">Debtpayment no:</fo:block>
					</fo:table-cell>
					<fo:table-cell>
						<fo:block font-weight="bold" text-align="right" id="fieldCount">xx</fo:block>
					</fo:table-cell>
				</fo:table-row>
			</fo:table-body>
		</fo:table>
	</fo:block>
</DIVFO>
