      <check name="FIN_FINACC_SHOWSWIFT_CHK"><![CDATA[(((UPPER((BANKFORMAT)) = UPPER('SWIFT')) AND (SWIFTCODE IS NOT NULL)) AND (GENERICACCOUNTNO IS NOT NULL)) OR (UPPER((BANKFORMAT)) <> UPPER('SWIFT'))]]></check>
