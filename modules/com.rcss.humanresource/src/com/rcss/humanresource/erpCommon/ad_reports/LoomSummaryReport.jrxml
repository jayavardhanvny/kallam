efficiency::numeric as Efficiency,
(select name from rcpl_loomcategory where rcpl_loomcategory_id= (select rcpl_loomcategory_id from rcpl_loom
where rcpl_loom_id=rcpl_prodincentiveline.rcpl_loom_id )) as LoomType,
(select employeename from rchr_employee where rchr_employee_id=$P{EmpName}) as empname
 from rcpl_prodincentiveline
 left join rcpl_prodincentive on rcpl_prodincentive.rcpl_prodincentive_id=rcpl_prodincentiveline.rcpl_prodincentive_id
 left join rchr_employee on rchr_employee.rchr_employee_id = rcpl_prodincentive.rchr_employee_id
 where rcpl_prodincentiveline.Date between $P{FromD} and $P{ToDate}
and 1=1
$P!{aux_empname}]]>
