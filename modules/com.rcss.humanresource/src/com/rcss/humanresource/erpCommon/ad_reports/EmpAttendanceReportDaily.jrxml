	<queryString>
		<![CDATA[select
rchr_employee.documentno as empno,
rchr_employee.employeename as empname,
rchr_employee.punchno as punchno,
rchr_employee.employeetype as employeetpye,
(select rchr_designation.name from rchr_designation where rchr_designation.rchr_designation_id=rchr_employee.rchr_designation_id) as designation,
(select rchr_emp_dept.name from rchr_emp_dept where rchr_emp_dept.rchr_emp_dept_id=rchr_employee.rchr_emp_dept_id) as department,

(select Count(present) from rchr_dailyattenddemo where present='Y' and isovertime='N' and isweeklyoff='N' and attdate Between rchr_attd_process.startdate And rchr_attd_process.enddate
and attdate Between $P{FromD} And $P{ToDate}
and rchr_dailyattenddemo.rchr_employee_id=rchr_employee.rchr_employee_id) as presendays,

(select Count(isweeklyoff) from rchr_dailyattenddemo where isweeklyoff='Y' and attdate Between rchr_attd_process.startdate And rchr_attd_process.enddate
and attdate Between $P{FromD} And $P{ToDate}
and rchr_dailyattenddemo.rchr_employee_id=rchr_employee.rchr_employee_id) as weeeklyoffdays,

(select Count(present) from rchr_dailyattenddemo where present='Y' and isweeklyoff='Y'  and attdate Between rchr_attd_process.startdate And rchr_attd_process.enddate
and attdate Between $P{FromD} And $P{ToDate}
and rchr_dailyattenddemo.rchr_employee_id=rchr_employee.rchr_employee_id) as WOP,

(select Count(present) from rchr_dailyattenddemo where present='N' and isweeklyoff='N' and attdate Between rchr_attd_process.startdate And rchr_attd_process.enddate
and attdate Between $P{FromD} And $P{ToDate}
and rchr_dailyattenddemo.rchr_employee_id=rchr_employee.rchr_employee_id) as absentdays,
(select name from rchr_emp_dept where rchr_emp_dept_id=$P{Dept_ID}) as parm_dept,
(select name from rchr_subdepartment where rchr_subdepartment_id=$P{SubDept_ID}) as parm_subdept,
(select name  from rchr_designation where rchr_designation.rchr_designation_id=$P{Designation_ID}) as parm_desg,
monthname,
enddate,
(processingdays) as totaldays,
 1  as oneday
from rchr_employee, rchr_attd_process
where  ((enddate between $P{FromD} And $P{ToDate})
OR  (startdate between $P{FromD} And $P{ToDate})) and rchr_employee.isactive='Y'
and 1=1
$P!{aux_empname}
$P!{aux_dept}
$P!{aux_subdept}
$P!{aux_emptype}
$P!{aux_designation}
ORDER BY startdate,rchr_employee.punchno]]>
	</queryString>
