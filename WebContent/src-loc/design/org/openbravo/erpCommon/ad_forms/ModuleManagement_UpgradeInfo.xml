<REPORT>
  <template file="ModuleManagement_UpgradeInfo.html" />
  
  <PARAMETER id="paramLanguage" name="language" default=""/>
  <PARAMETER id="paramKey" name="key" default="" attribute="value"/>
  <PARAMETER id="paramDirectory" name="directory" default=""/>
  <PARAMETER id="paramCSS" name="theme" attribute="href" replace="Default"/>
  
  <PARAMETER id="messageBoxID" name="messageType" attribute="class" replace="yy" default="HIDDEN"/>
  <PARAMETER id="messageBoxIDTitle" name="messageTitle" default=""/>
  <PARAMETER id="messageBoxIDMessage" name="messageMessage" default=""/>
  
  <PARAMETER id="inpcUpdate" name="moduleID" default="" attribute="value"/>
  <PARAMETER id="upgradeVersion" name="upgradeVersion" default="" attribute="value"/>
 
  <PARAMETER id="upgradeName"  name="upgradeName"       default=""/>
  <PARAMETER id="upgradeInfo"  name="upgradeInfo"    default=""/>
  
   <structure name="updateNeeded">
    <FIELD id="updateMoule">name</FIELD>
    <FIELD id="updateIntalled">installed</FIELD>
    <FIELD id="updateRequired">required</FIELD>
    <FIELD id="updateMaturityMsg" attribute="style" replace="none" default="none">displayMaturity</FIELD>
    <FIELD id="updateMaturity" default="">mat</FIELD>
    <SECTION id="updateNeeded"/>
   </structure>
</REPORT>
