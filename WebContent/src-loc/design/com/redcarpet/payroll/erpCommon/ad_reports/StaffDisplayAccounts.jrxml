	<queryString>
		<![CDATA[SELECT
emp.documentno AS Employeeid,

emp.employeename AS name,
( CASE WHEN $P{Dyieng}='Y' THEN 'DYEING'
 WHEN $P{Dyieng}='N' THEN 'WEAVING'
  WHEN $P{Dyieng}='' THEN 'WEAVING + DYEING'
 END) as branch,
(SELECT rchr_designation.name FROM rchr_designation
WHERE rchr_designation_id=emppp.rchr_designation_id ) AS designation,

(SELECT RCPL_EmpPaydead.deductions from RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='PF')) AS PF,

(SELECT RCPL_EmpPaydead.deductions from RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='ESI')) AS ESI,

(SELECT RCPL_EmpPaydead.deductions FROM RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id  AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Electricity Bill')) AS ElectricityBill,


(SELECT RCPL_EmpPaydead.deductions FROM RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id  AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Late Attendance Disincentive')) AS LateAttendanceDisincentive,


COALESCE((select sum(sum) from RCPL_MessBill,rcpl_messtype where RCPL_MessBill.rchr_employee_id=emp.rchr_employee_id and
fromdate between  pp.startdate and pp.enddate and
todate between  pp.startdate and pp.enddate and RCPL_MessBill.rcpl_messtype_id=rcpl_messtype.rcpl_messtype_id and rcpl_messtype.name='Baba Mess' ),0) as babamess,

COALESCE((select sum(sum) from RCPL_MessBill,rcpl_messtype where RCPL_MessBill.rchr_employee_id=emp.rchr_employee_id and
fromdate between  pp.startdate and pp.enddate and
todate between  pp.startdate and pp.enddate and RCPL_MessBill.rcpl_messtype_id=rcpl_messtype.rcpl_messtype_id and rcpl_messtype.name='BQ Mess' ),0) as bqmess,

COALESCE((select sum(sum) from RCPL_MessBill,rcpl_messtype where RCPL_MessBill.rchr_employee_id=emp.rchr_employee_id and
fromdate between  pp.startdate and pp.enddate and
todate between  pp.startdate and pp.enddate and RCPL_MessBill.rcpl_messtype_id=rcpl_messtype.rcpl_messtype_id and rcpl_messtype.name='Ladies Hostel Mess' ),0) as ladiesmess,

COALESCE((select sum(sum) from RCPL_MessBill,rcpl_messtype where RCPL_MessBill.rchr_employee_id=emp.rchr_employee_id and
fromdate between  pp.startdate and pp.enddate and
todate between  pp.startdate and pp.enddate and RCPL_MessBill.rcpl_messtype_id=rcpl_messtype.rcpl_messtype_id and rcpl_messtype.name='Managers Mess' ),0) as mgrmess,
(select sum(sum) from RCPL_MessBill,rcpl_messtype where RCPL_MessBill.rchr_employee_id=emp.rchr_employee_id and
fromdate between  pp.startdate and pp.enddate and
todate between  pp.startdate and pp.enddate and RCPL_MessBill.rcpl_messtype_id=rcpl_messtype.rcpl_messtype_id and rcpl_messtype.name='Managers Mess' ) as mgrmess,


(SELECT RCPL_EmpPaydead.deductions from RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='PT')) AS PT,

(SELECT RCPL_EmpPaydead.deductions from RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Other Deductions')) AS OtherDeductions,


(SELECT RCPL_EmpPaydead.deductions from RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Gas')) AS GasRecovery,


(SELECT RCPL_EmpPaydead.deductions from RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Employee Fine')) AS Fine,


(SELECT RCPL_EmpPaydead.deductions FROM RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND
RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Welfare Fund')) AS welfare,

(SELECT RCPL_EmpPaydead.deductions FROM RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND
RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Store Recovery')) AS Deptstore,


(SELECT RCPL_EmpPaydead.deductions FROM RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND
RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Security Deposit')) AS secdeposite,


(SELECT RCPL_EmpPaydead.deductions from RCPL_EmpPaydead WHERE emppp.rcpl_emppayrollprocess_id=RCPL_EmpPaydead.rcpl_emppayrollprocess_id AND RCPL_EmpPaydead.rcpl_paydeducitionshead_id=(SELECT rcpl_paydeducitionshead_id FROM
rcpl_paydeducitionshead WHERE category='Rent Bill')) AS RentBill,


COALESCE((select sum(loanamount) from rchr_emp_loanlines loanlines left join rchr_emp_loan loans on loans.rchr_emp_loan_id=loanlines.rchr_emp_loan_id
left join rchr_loan_type loantype on loantype.rchr_loan_type_id= loans.rchr_loan_type_id
where loans.rchr_employee_id=emp.rchr_employee_id  and
loantype.value='Advance' and loanlines.paid='Y' and
loanlines.duedate between pp.startdate and pp.enddate
),0) as advance,

COALESCE( (select sum(loanamount) from rchr_emp_loanlines loanlines left join rchr_emp_loan loans on loans.rchr_emp_loan_id=loanlines.rchr_emp_loan_id
left join rchr_loan_type loantype on loantype.rchr_loan_type_id= loans.rchr_loan_type_id
where loans.rchr_employee_id=emp.rchr_employee_id  and
loantype.value = 'Loan' and loanlines.paid='Y' and
loanlines.duedate between pp.startdate and pp.enddate
),0)  as loan,
COALESCE( (select sum(loanamount) from rchr_emp_loanlines loanlines left join rchr_emp_loan loans on loans.rchr_emp_loan_id=loanlines.rchr_emp_loan_id
left join rchr_loan_type loantype on loantype.rchr_loan_type_id= loans.rchr_loan_type_id
where loans.rchr_employee_id=emp.rchr_employee_id  and
loantype.value = 'Special' and loanlines.paid='Y' and
loanlines.duedate between pp.startdate and pp.enddate
),0)  as specialloan,


(SELECT rchr_attd_process.monthname FROM rchr_attd_process WHERE rchr_attd_process.rchr_attd_process_id=pp.rchr_attd_process_id) AS PayrollProcessMonth,

emppp.deductionsum*-1 AS totaldeduction,


emppp.grandtotal as grandtotal,



0 as zero,

round(emppp.totalpay) AS netsal,

(select name from rchr_emp_dept where rchr_emp_dept_id=$P{Dept_ID}) as parm_dept,

('- '||(select name from rchr_subdepartment where rchr_subdepartment_id=$P{SubDept_ID})) as parm_subdept
,pp.status
FROM rcpl_payrollprocess pp
left join rcpl_emppayrollprocess emppp
ON emppp.rcpl_payrollprocess_id = pp.rcpl_payrollprocess_id
left join rchr_employee emp on emp.rchr_employee_id=emppp.rchr_employee_id
left join rchr_emp_dept dept on dept.rchr_emp_dept_id=emp.rchr_emp_dept_id
where pp.rchr_attd_process_id=$P{ProcessID} AND emp.employeetype=$P{EmpType}
and pp.isactive='Y' and emppp.totalpay >0
and 1=1
$P!{aux_emp}
$P!{aux_dept}
$P!{aux_subdept}
$P!{aux_emp_pf}
$P!{aux_emp_bank}
$P!{aux_dyieng}
$P!{aux_documentno}
ORDER BY emp.documentno]]>
	</queryString>
