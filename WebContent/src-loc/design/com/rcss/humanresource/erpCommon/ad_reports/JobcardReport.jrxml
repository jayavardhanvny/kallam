	<parameter name="jobcard" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
 sort.qualityno as sortno,
 job.orderno as orderno,
 job.documentno as jobno,
 job.mofdate as jobdate,
 sort.qstandard as constuction,
 job.orderquantity as ordqty,
 job.deliverydate as deliverydate,
 (sort.warpcount||' '||(select name from rchr_warpyarntype where rchr_warpyarntype_id=sort.rchr_warpyarntype_id)||
 coalesce((select name from epcg_variant where epcg_variant_id=sort.epcg_variant_id),'')) as warpcount,
 job.warpyarnreq,
 (sort.weftcount||' '||(select name from rchr_warpyarntype where rchr_warpyarntype_id=sort.warpyarntype) ||
coalesce((select name from epcg_variant where epcg_variant_id=sort.weftvariant),'')) as weftcount,
 job.weftyarnreq,
 sort.reedspace,
 (select name||' "' from rchr_inspgreigewidth where rchr_inspgreigewidth_id=sort.rchr_inspgreigewidth_id) as greig_width,
 sort.reed,
 sort.epi,
 sort.ppi,
 sort.gsm,
 sort.glm,
 sort.warpcrimp as warpcrimp,
 job.towarpdate,
 job.towrapmtrs,
 job.sizingdate,
 job.toloomdate,
 job.nooflloms,
 job.warpcustomer,
 job.weftcustomer,
 sort.selvedents,
 sort.selveends,
 sort.selvedge,
  sort.fulllengthper ,
  sort.fulllengthmtr ,
  sort.shortlengthper ,
  sort.shortlengthmtr ,
  sort.baleorrole ,
  sort.balesize ,
  sort.totalends ,
  sort.warptearingstrngth ,
  sort.wefttearingstrngth ,
  sort.tearingtestmethod ,
  sort.warptensiblestrngth ,
  sort.wefttensiblestrngth ,
  sort.coverfactor,
  sort.selvweave,
( select Name from rchr_orderstatus where rchr_orderstatus_id=job.rchr_orderstatus_id) as order_Type,
  sort.tensibletestmethod ,
 ( select code from rchr_warpweftclolor where rchr_warpweftclolor_id=sort.rchr_warpweftclolor_id) as warpcolor,
 ( select code from rchr_warpweftclolor where rchr_warpweftclolor_id=sort.weftcolor) as weftcolor,
 (select binarydata from ad_image where ad_image_id  = (select ad_image_id from rchr_inspweave where rchr_inspweave_id=sort.rchr_inspweave_id)) AS bynary,
  job.description as remarks

 from  rchr_jobcard job
 left join rchr_qualitystandard sort on sort.rchr_qualitystandard_id=job.rchr_qualitystandard_id
where job.rchr_jobcard_id=$P{jobcard}]]>
	</queryString>
