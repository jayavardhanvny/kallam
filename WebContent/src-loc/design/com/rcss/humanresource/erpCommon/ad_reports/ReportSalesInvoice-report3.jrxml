	<queryString>
		<![CDATA[select c_invoice.c_invoice_id,c_invoice.documentno,dateinvoiced, c_bpartner.name as businesspartner,c_paymentterm.name as payment,

 ((select COALESCE(c_location.address1,'') from c_location where c_location.c_location_id=c_bpartner_location.c_location_id)   ||
 (select COALESCE(c_location.address2,'') from c_location where c_location.c_location_id=c_bpartner_location.c_location_id) ||
 (select COALESCE(c_location.city,'') from c_location where c_location.c_location_id=c_bpartner_location.c_location_id) ||','||
(select COALESCE(name,'') from c_region where c_region.c_region_id=(select c_region_id from c_location where c_location.c_location_id=c_bpartner_location.c_location_id)) ||'-'||
 (select COALESCE(c_location.postal,'') from c_location where c_location.c_location_id=c_bpartner_location.c_location_id) ||','||
 (select COALESCE(name,'') from c_country where c_country.c_country_id=(select c_country_id from c_location where c_location.c_location_id=c_bpartner_location.c_location_id))
   )  as location


,Round((c_invoiceline.line/10),0) as line,
c_invoiceline.taxamt,c_invoiceline.qtyinvoiced,c_invoiceline.pricelist,c_invoice.description,
Round(c_invoice.grandtotal,0) as grandtotal,(SELECT Documentno FROm c_order Where c_order_id = (Select  c_order_id FROM c_orderline Where c_orderline_id = (SELECT c_orderline_id FROM c_invoiceline Where c_invoiceline.c_invoice_id = c_invoice.c_invoice_id and c_invoiceline.line = '10'  Limit 1)Limit 1)Limit 1) as c_order_id ,
c_invoiceline.chargeamt,m_product.name as product,c_invoiceline.Financial_Invoice_Line,c_bpartner.EM_Epcg_Tin as Tinno,
c_bpartner.EM_Epcg_Cst as Cstno,

Case WHEN c_invoiceline.Financial_Invoice_Line = 'N'   THEN
( CASE WHEN c_invoiceline.epcg_packaging_id is Null THEN
(Select name from m_product Where c_invoiceline.m_product_id = m_product.m_product_id)
ELSE
(
Case WHEN c_invoiceline.em_epcg_prolistalias is Null THEN

((Select name from m_product Where c_invoiceline.m_product_id = m_product.m_product_id) ||
(Select name from epcg_packaging Where c_invoiceline.epcg_packaging_id =epcg_packaging_id))
ELSE

((Select Value from m_product Where c_invoiceline.m_product_id = m_product.m_product_id) || ' ' || em_epcg_prolistalias ||
(Select name from epcg_packaging Where c_invoiceline.epcg_packaging_id =epcg_packaging_id))
END
)END)
Else
(Select name from C_Glitem Where c_invoiceline.Account_ID =C_Glitem.C_Glitem_ID) END as pack,


c_uom.name as UOM,c_invoiceline.pricelist as pricelist,c_invoiceline.linenetamt as linenetamount,
c_invoice.totallines as linetotal,Round((select sum(taxamt) from c_invoicetax where c_invoicetax.c_invoice_id=c_invoice.c_invoice_id),0) as taxamount,
c_invoice.em_epcg_epcgone as epcgid,(select epcglicenseno from epcg_epcgnew where epcg_epcgnew.epcg_epcgnew_id=c_invoice.em_epcg_epcgone) as epcglno,
(select name as agentname from c_bpartner where c_bpartner_id=c_invoice.em_epcg_agent limit 1)  as agentname,em_epcg_lcno,
em_epcg_lcdate,em_epcg_waybillno,em_epcg_portload,em_epcg_portdischarge,em_epcg_finaldest,
em_epcg_lrno,em_epcg_transname,em_epcg_areno,em_epcg_exciseno,em_epcg_remarks,em_epcg_edexempt,
em_epcg_exporttype,em_epcg_domestictype,em_epcg_invoicetype,c_invoice.ad_org_id as orgid, (select ad_orginfo.c_location_id where c_invoice.ad_org_id=ad_orginfo.ad_org_id) as locid,
(select c_location.address1 where ad_orginfo.c_location_id=c_location.c_location_id) as orgaddr1,
(select c_location.address2 where ad_orginfo.c_location_id=c_location.c_location_id) as orgaddr2,
(select c_location.city where ad_orginfo.c_location_id=c_location.c_location_id) as orgcity,
(select c_location.postal where ad_orginfo.c_location_id=c_location.c_location_id) as orgpostal,
(select ad_org.em_epcg_tin from ad_org where c_invoice.ad_org_id=ad_org.ad_org_id) as orgtin,
(select ad_org.em_epcg_division from ad_org where c_invoice.ad_org_id=ad_org.ad_org_id) as orgdivision,
(select ad_org.em_epcg_iec from ad_org where c_invoice.ad_org_id=ad_org.ad_org_id) as orgiec,
(select ad_org.em_epcg_pan from ad_org where c_invoice.ad_org_id=ad_org.ad_org_id) as orgpan,
(select ad_org.em_epcg_ecc from ad_org where c_invoice.ad_org_id=ad_org.ad_org_id) as orgecc,
(select ad_org.em_epcg_excise from ad_org where c_invoice.ad_org_id=ad_org.ad_org_id) as orgexcise, c_invoiceline.em_epcg_noofpacks as noofpacks,c_invoiceline.description as description,
c_invoiceline.priceactual as priceperunit,c_invoice.c_order_id, c_order.documentno as orderno, c_invoice.POReference as orderreference,
(select name from c_tax where c_tax.c_tax_id=c_invoiceline.c_tax_id) as taxname,(select SUM(taxamt) from c_invoicelinetax where c_invoicelinetax.c_invoiceline_id=c_invoiceline.c_invoiceline_id) as taxamountnew,
c_invoice.em_epcg_locdelivery,c_invoiceline.Account_ID, C_Glitem.name as accname, CAse When c_invoice.em_epcg_type is Null Then c_invoice.em_epcg_type Else
'Against ' || c_invoice.em_epcg_type End as formtype,c_invoiceline.EM_Epcg_Noofpackages as newnopacks,FIN_Paymentmethod.description as paymethdes,(SELECT printName FROM C_DocType WHERE C_DocType_id = c_invoice.C_DocTypeTarget_ID limit 1) print

from c_invoice
              left join c_bpartner on c_invoice.c_bpartner_id = c_bpartner.c_bpartner_id
              left join c_paymentterm on c_invoice.c_paymentterm_id = c_paymentterm.c_paymentterm_id
              left join c_bpartner_location on c_invoice.c_bpartner_location_id = c_bpartner_location.c_bpartner_location_id
              left join c_invoiceline on c_invoice.c_invoice_id = c_invoiceline.c_invoice_id
              left join m_product on c_invoiceline.m_product_id = m_product.m_product_id
              left join c_uom on c_invoiceline.c_uom_id = c_uom.c_uom_id
              left join ad_orginfo on c_invoice.ad_org_id = ad_orginfo.ad_org_id
              left join c_location on ad_orginfo.c_location_id = c_location.c_location_id
              left join FIN_Paymentmethod on c_invoice.FIN_Paymentmethod_ID = FIN_Paymentmethod.FIN_Paymentmethod_ID
              left join c_order on c_order.c_order_id = c_invoice.c_order_id
              left join C_Glitem on c_invoiceline.Account_ID = C_Glitem.C_Glitem_ID


              where c_invoice.issotrx='Y' and c_invoice.c_invoice_id = $P{C_INVOICE_ID}]]>
	</queryString>
