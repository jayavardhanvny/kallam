	<queryString>
		<![CDATA[select rchr_employee.documentno as empno,
rchr_employee.employeename as empname,rchr_employee.punchno as punchno,rchr_employee.employeetype as employeetpye,
(select employeename from rchr_employee where rchr_employee_id=$P{EmpName} )as parm_emp,

(select rchr_designation.name from rchr_designation where rchr_designation.rchr_designation_id=rchr_employee.rchr_designation_id) as designation,
(select rchr_emp_dept.name from rchr_emp_dept where rchr_emp_dept.rchr_emp_dept_id=rchr_employee.rchr_emp_dept_id) as department,

now() as currentdate,
subtable.presentscount as presendays, subtable.attendancedate,

(select name from rchr_emp_dept where rchr_emp_dept_id=$P{Dept_ID}) as parm_dept,
(select name from rchr_subdepartment where rchr_subdepartment_id=$P{SubDept_ID}) as parm_subdept,
(select name  from rchr_designation where rchr_designation.rchr_designation_id=$P{Designation_ID}) as parm_desg,
 1  as oneday
from rchr_employee
left join (select coalesce(Count(present),0) as presentscount, demo.attdate as attendancedate, demo.rchr_employee_id as empid
from rchr_dailyattenddemo demo where demo.present='Y' and demo.isweeklyoff='N' AND demo.isovertime=$P{presentOrOt} and demo.attdate Between $P{FromD} And $P{ToDate}
 group by demo.rchr_employee_id, demo.attdate) subtable
on subtable.empid=rchr_employee.rchr_employee_id
where rchr_employee.employeestatus='W'
and subtable.presentscount is not null
and subtable.presentscount>1
and 1=1
$P!{aux_empname}
$P!{aux_emptype}
$P!{aux_dept}
$P!{aux_subdept}
$P!{aux_designation}
ORDER BY rchr_employee.punchno]]>
	</queryString>
